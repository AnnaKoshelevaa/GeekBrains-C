int [,]pic = new int [23,25];

void PrintImage(int [,] image);
{
    for (int i =0; i< image.GetLenght(0); i++)
    {
       for (int j =0; j< image.GetLenght(1); j++)
         {
        //Console.Write($"{image[i;j]}");

        if (image [i;j]==0) Console.Write($" ");
        else Console.Write($"+");
         };
    Console.WriteLine();
    };

};

PrintImage(pic)
